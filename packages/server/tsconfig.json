{
  "compilerOptions": {
    "target": "es2018",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": "."
  },
  "exclude": ["node_modules"],
  "include": ["./**/*.tsx", "./**/*.ts", "src/configs/test.ts"]
}
